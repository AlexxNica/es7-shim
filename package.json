{
	"name": "es7-shim",
	"version": "0.0.0",
	"description": "ECMAScript 7 compatibility shims for legacy JavaScript engines",
	"homepage": "http://github.com/es-shims/es7-shim/",
	"contributors": [
		"Jordan Harband <ljharb@gmail.com> (https://github.com/ljharb/)"
	],
	"bugs": {
		"mail": "ljharb@gmail.com",
		"url": "http://github.com/es-shims/es7-shim/issues"
	},
	"licenses": [
		{
			"type": "MIT",
			"url": "http://github.com/es-shims/es7-shim/raw/master/LICENSE"
		}
	],
	"main": "es7-shim.js",
	"browser": "es7-shim.browser.js",
	"repository": {
		"type": "git",
		"url": "http://github.com/es-shims/es7-shim.git"
	},
	"scripts": {
		"test": "node test/*.js",
		"prepublish": "npm run minify",
		"build": "mkdir -p dist && browserify es7-shim.js > dist/es7-shim.js",
		"minify": "npm run build && uglifyjs dist/es7-shim.js --comments --source-map=dist/es7-shim.map -m -b ascii_only=true,beautify=false > dist/es7-shim.min.js"
	},
	"dependencies": {
		"array-includes": "~1.0.3"
	},
	"devDependencies": {
		"tape": "~3.0.3",
		"runsync": "~0.1.8",
		"browserify": "~8.0.3",
		"uglify-js": "~2.4.16"
	},
	"engines": {
		"node": ">=0.4.0"
	},
	"testling": {
		"browsers": [
			"iexplore/6.0..latest",
			"firefox/3.0..6.0",
			"firefox/18.0..latest",
			"firefox/nightly",
			"chrome/4.0..10.0",
			"chrome/25.0..latest",
			"chrome/canary",
			"opera/10.0..latest",
			"opera/next",
			"safari/4.0..latest",
			"ipad/6.0..latest",
			"iphone/6.0..latest",
			"android-browser/4.2"
		]
	}
}

